{
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  {
    "plugins": ["jest"]
  },
  "rules": {
      "semi": 1,
      "indent": ["warn", 2]
  }
}